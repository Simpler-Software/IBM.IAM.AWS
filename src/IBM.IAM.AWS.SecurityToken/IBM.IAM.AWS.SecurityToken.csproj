<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>IBM.IAM.AWS.SecurityToken</AssemblyName>
    <Description>AWS SAML integration with IBM's Identity and Access Management</Description>
    <Product>IBM - Identity and Access Management SAML</Product>
    <Version>1.0.2002</Version>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Authors>John W Carew</Authors>
    <Company>Simpler Software</Company>
    <Copyright>(c) 2019 Simpler Software. All rights reserved.</Copyright>
    <PackageTags>AWS;SAML;IBM;IAM</PackageTags>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Simpler-Software/IBM.IAM.AWS</PackageProjectUrl>
    <PackageIconUrl>https://sdk-for-net.amazonwebservices.com/images/AWSLogo128x128.png</PackageIconUrl>
    <AssemblyVersion>1.0.2002.66</AssemblyVersion>
    <FileVersion>1.0.2002.66</FileVersion>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <XmlDoc2CmdletDocStrict>true</XmlDoc2CmdletDocStrict>
    <RunPostBuildEvent>Always</RunPostBuildEvent>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3">
    </PackageReference>
    <PackageReference Include="PowerShellStandard.Library" Version="5.1.0">
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
    <PackageReference Include="XmlDoc2CmdletDoc" Version="0.2.13">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <None Update="IBM.IAM.AWS.SecurityToken.Format.ps1xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="IBM.IAM.AWS.SecurityToken.psd1">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="LICENSE.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="REM IF DEFINED CI exit&#xD;&#xA;&#xD;&#xA;for /f &quot;delims=&quot; %25%25A in ('powershell -NoProfile -Command &quot;$v = ..\..\build\Get-ModuleManifestVersion.ps1 -ManifestPath .\$(TargetName).psd1;$env:Version&quot;') do set &quot;PSModuleVersion=%25%25~A&quot;&#xD;&#xA;set module=&quot;$(TargetName)&quot;&#xD;&#xA;if defined PSModuleVersion set module=%25module%25\%25PSModuleVersion%25&#xD;&#xA;&#xD;&#xA;set &quot;Personal=&quot;&#xD;&#xA;for /f &quot;delims=&quot; %25%25A in ('powershell -NoProfile -Command &quot;[Environment]::GetFolderPath('MyDocuments') | Out-String&quot;') do set &quot;Personal=%25%25~A&quot;&#xD;&#xA;if not defined Personal exit /b 1&#xD;&#xA;echo Coping to %25Personal%25\WindowsPowerShell\Modules\%25module%25\&#xD;&#xA;&#xD;&#xA;del /Q &quot;%25Personal%25\WindowsPowerShell\Modules\%25module%25\*.*&quot;&#xD;&#xA;xcopy  /Y &quot;$(TargetDir)*.dll&quot; &quot;%25Personal%25\WindowsPowerShell\Modules\%25module%25\&quot;&#xD;&#xA;xcopy  /Y &quot;$(TargetDir)*.psd1&quot; &quot;%25Personal%25\WindowsPowerShell\Modules\%25module%25\&quot;&#xD;&#xA;xcopy  /Y &quot;$(TargetDir)*.ps1xml&quot; &quot;%25Personal%25\WindowsPowerShell\Modules\%25module%25\&quot;&#xD;&#xA;xcopy  /Y &quot;$(TargetDir)*.txt&quot; &quot;%25Personal%25\WindowsPowerShell\Modules\%25module%25\&quot;&#xD;&#xA;xcopy  /Y &quot;$(TargetDir)*Help.xml&quot; &quot;%25Personal%25\WindowsPowerShell\Modules\%25module%25\&quot;" />
  </Target>
</Project>